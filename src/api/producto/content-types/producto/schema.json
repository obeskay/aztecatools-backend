{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "descripcion": {
      "type": "richtext"
    },
    "imagenes": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "portada": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "precio": {
      "type": "decimal"
    },
    "variantes": {
      "type": "component",
      "repeatable": true,
      "component": "variantes.variante"
    },
    "slug": {
      "type": "uid",
      "targetField": "nombre",
      "required": true
    },
    "stock": {
      "type": "integer",
      "min": 0
    },
    "precioComparacion": {
      "type": "decimal"
    },
    "stars": {
      "type": "integer",
      "max": 5,
      "min": 1
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "productos"
    },
    "subcategoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategoria.subcategoria",
      "inversedBy": "productos"
    }
  }
}
